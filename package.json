{
  "name": "kitoon",
  "version": "0.0.1",
  "dependencies": {
    "angular": "~1.4.3",
    "angular-resource": "~1.4.3",
    "angular-cookies": "~1.4.3",
    "angular-sanitize": "~1.4.3",
    "angular-route": "~1.4.3",
    "angular-animate": "~1.4.3",
    "angular-strap": "~2.3.1",
    "numeral": "~1.5.3",
    "idb-wrapper": "~1.5.0",
    "lodash": "~3.10.0",
    "font-awesome": "~4.5.0",
    "patternfly": "~2.10.0",
    "angular-motion": "~0.4.2",
    "angular-growl-v2": "~0.7.5",
    "d3": "~3.5.6",
    "c3": "~0.4.11-rc3",
    "jquery": "~1.11.3",
    "es6-shim": "~0.33.6",
    "restangular": "git://github.com/mgonto/restangular#1.5.1",
    "c3-angular": "git://github.com/jettro/c3-angular-directive#0.7.1"
  },
  "devDependencies": {
    "browserify": "~12.0.2",
    "browserify-shim": "~3.8.9",
    "del": "*",
    "gulp": "~3.9.0",
    "gulp-concat": "~2.6.0",
    "gulp-debug": "*",
    "gulp-if": "~2.0.0",
    "gulp-inject": "*",
    "gulp-load-plugins": "*",
    "gulp-ng-annotate": "*",
    "gulp-sass": "*",
    "gulp-sourcemaps": "*",
    "gulp-typescript": "*",
    "gulp-uglify": "~1.4.1",
    "merge-stream": "^1.0.0",
    "requirejs": "*",
    "typescript": "~1.7.3",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "*",
    "yargs": "~3.31.0"
  },
  "browser": {
    "restangular": "./node_modules/restangular/dist/restangular.min.js",
    "c3-angular": "./node_modules/c3-angular/c3-angular.min.js",
    "angular-strap": "./node_modules/angular-strap/dist/angular-strap.js",
    "angular-strap-tpl": "./node_modules/angular-strap/dist/angular-strap.tpl.js",
    "angular-growl": "./node_modules/angular-growl-v2/build/angular-growl.js",
    "idb-wrapper": "./node_modules/idb-wrapper/idbstore.js",
    "patternfly": "./node_modules/patternfly/dist/js/patternfly.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "restangular": {
      "depends": [
        "angular",
        "lodash: _"
      ]
    },
    "c3-angular": {
      "depends": [
        "angular",
        "d3",
        "c3: c3"
      ]
    },
    "angular-strap": {
      "depends": [
        "angular"
      ]
    },
    "angular-strap-tpl": {
      "depends": [
        "angular-strap"
      ]
    },
    "patternfly": {
      "depends": [
        "jquery: jQuery"
      ]
    }
  }
}
